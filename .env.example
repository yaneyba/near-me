# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Slack Webhook URL for notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Admin API Authentication
# Generate a secure token: openssl rand -base64 32
ADMIN_API_TOKEN=your-secure-admin-token-here

# Authentication (when enabled)
VITE_SETTINGS_AUTH_LOGIN_ENABLED=false

# Feature Flags
VITE_SETTINGS_ENABLE_TRACKING=true
VITE_SETTINGS_ENABLE_ADS=false

# Database
# Note: Cloudflare D1 bindings are configured in wrangler.toml

# Stripe (for payments)
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here

# Allowed CORS Origins (comma-separated)
ALLOWED_ORIGINS=https://near-me.us,https://*.near-me.us

# Environment
NODE_ENV=development
